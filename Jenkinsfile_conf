pipeline {
    agent any
    stages {
        stage('Get_Code') {
            steps {
                git branch: 'develop', url:'https://github.com/beetlebum97/todo-list-aws.git'
                sh '''
                    rm -f samconfig.toml
                    curl -O https://raw.githubusercontent.com/beetlebum97/todo-list-aws-config/staging/samconfig.toml
                    ls -la
                    cat samconfig.toml
                '''
            }
        }
    }   
    post {
        always {
            script {
                cleanWs()
            }
        }
    }
}
